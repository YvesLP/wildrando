<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}WildRando{% endblock %}</title>
        {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="{{ asset('/wildrandoCss/bootstrap.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('/wildrandoCss/footer.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('/wildrandoCss/styles.css') }}">
        {% endblock %}
        {% block javascripts %}
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>
        <script async defer src="https://maps.googleapis.com/maps/api/js?key=YOUR_API_KEY&callback=initMap"></script>
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div class="container-fluid wrapper">
        {% block menu %}{% endblock %}

            {% block body %}{% endblock%}

        {% block footer %}{% endblock %}
        </div>
    </body>
</html>
